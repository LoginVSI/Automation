    "RetryPolicy": {
      "MaxRetries": 10,
      "DelayInMs": 1000,
      "RetryJitterInMs": 10,
      "ExponentialDelayBase": 1.5
    }
  },
  "TokenManager": {
    "ClientCredentials": {
      "ClientId": "Launcher",
      "ClientSecret": "sdfsdf99BEA97594CEA5B3700",
      "Scope": "microservice"
    }
  },
  "MicroserviceClient": {
    "RetryPolicy": {
      "MaxRetries": 10,
      "DelayInMs": 1000,
      "RetryJitterInMs": 10,
      "ExponentialDelayBase": 1.5
    },
    "Logging": {
      "WarningThresholdInMilliseconds": 1000
    }
  }
}